<UserControl
    x:Class="TileShop.AvaloniaUI.Views.NameResourceView"
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:vm="using:TileShop.AvaloniaUI.ViewModels"
    x:CompileBindings="True"
    x:DataType="vm:NameResourceViewModel">
    <Grid>
        <StackPanel>
            <TextBox MinWidth="220" FontSize="20" Text="{Binding ResourceName}" />
        </StackPanel>
    </Grid>
</UserControl>
