<format name="Virtual Boy 2bpp" version="0.8">
    <codec>
        <colortype>indexed</colortype>
        <colordepth>2</colordepth>
        <layout>tiled</layout>
        <defaultheight>8</defaultheight>
        <defaultwidth>8</defaultwidth>
        <fixedsize>true</fixedsize>
		<mergepriority>0, 1</mergepriority>
    </codec>
    <image>
        <colordepth>2</colordepth>
		<rowpixelpattern>3, 2, 1, 0, 7, 6, 5, 4</rowpixelpattern>
        <rowinterlace>false</rowinterlace>
    </image>
</format>