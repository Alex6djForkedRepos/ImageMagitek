<format name="SNES 3bpp">
    <codec>
        <colortype>indexed</colortype>
        <colordepth>3</colordepth>
        <layout>tiled</layout>
        <defaultheight>8</defaultheight>
        <defaultwidth>8</defaultwidth>
        <fixedsize>false</fixedsize>
		<mergepriority>0, 1, 2</mergepriority>
    </codec>
    <image>
        <colordepth>2</colordepth>
        <rowinterlace>true</rowinterlace>
    </image>
    <image>
        <colordepth>1</colordepth>
        <rowinterlace>true</rowinterlace>
    </image>
</format>